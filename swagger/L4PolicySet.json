            "type": "object", 
            "properties": {
                "start": {
                    "type": "integer", 
                    "description": "TCP/UDP port range start (inclusive). Allowed values are 1-65535.", 
                    "format": "int32"
                }, 
                "end": {
                    "type": "integer", 
                    "description": "TCP/UDP port range end (inclusive). Allowed values are 1-65535.", 
                    "format": "int32"
                }
            }
