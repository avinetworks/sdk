    "definitions": {
        "MicroService": {
            "required": [
                "application_name", 
                "name", 
                "orchestrator_name"
            ], 
            "type": "object", 
            "properties": {
                "orchestrator_name": {
                    "type": "string", 
                    "description": "orchestrator_name of MicroService."
                }, 
                "_last_modified": {
                    "readOnly": true, 
                    "type": "string", 
                    "description": "UNIX time since epoch in microseconds. Units(MICROSECONDS)."
                }, 
                "url": {
                    "readOnly": true, 
                    "type": "string", 
                    "description": "url"
                }, 
                "description": {
                    "type": "string", 
                    "description": "User defined description for the object."
                }, 
                "tenant_ref": {
                    "type": "string", 
                    "description": " It is a reference to an object of type Tenant."
                }, 
                "created_by": {
                    "type": "string", 
                    "description": "Creator name."
                }, 
                "name": {
                    "type": "string", 
                    "description": "Name of the object."
                }, 
                "cloud_config_cksum": {
                    "type": "string", 
                    "description": "Checksum of cloud configuration for Microservice. Internally set by cloud connector. Field introduced in 17.2.8."
                }, 
                "ip_list": {
                    "type": "boolean", 
                    "description": "Flag to indicate if container IP list is provided by cloud connectorThis is applicable for overlay cases."
                }, 
                "application_name": {
                    "type": "string", 
                    "description": "application_name of MicroService."
                }, 
                "containers": {
                    "items": {
                        "$ref": "#/definitions/MicroServiceContainer"
                    }, 
                    "type": "array", 
                    "description": "The list of containers for this microservice."
                }, 
                "uuid": {
                    "type": "string", 
                    "description": "Unique object identifier of the object."
                }
            }
        }, 
        "MicroServiceContainer": {
            "required": [
                "ip"
            ], 
            "type": "object", 
            "properties": {
                "ip": {
                    "description": "IP Address of the container.", 
                    "$ref": "#/definitions/IpAddr"
                }, 
                "host": {
                    "type": "string", 
                    "description": "ID or name of the host where the container is."
                }, 
                "container_id": {
                    "type": "string", 
                    "description": "ID of the container."
                }, 
                "port": {
                    "type": "integer", 
                    "description": "Port nunber of the instance.", 
                    "format": "int32"
                }, 
                "task_id": {
                    "type": "string", 
                    "description": "Marathon Task ID of the instance."
                }
            }
        }, 
        "IpAddr": {
            "required": [
                "addr", 
                "type"
            ], 
            "type": "object", 
            "properties": {
                "type": {
                    "type": "string", 
                    "description": " Enum options - V4, DNS, V6."
                }, 
                "addr": {
                    "type": "string", 
                    "description": "IP address."
                }
            }
        }, 
        "MicroServiceApiResponse": {
            "required": [
                "count", 
                "results"
            ], 
            "type": "object", 
            "properties": {
                "count": {
                    "type": "integer", 
                    "format": "int32"
                }, 
                "results": {
                    "items": {
                        "$ref": "#/definitions/MicroService"
                    }, 
                    "type": "array"
                }
            }
        }
    }, 
