    "definitions": {
        "AlertSyslogServer": {
            "required": [
                "syslog_server", 
                "udp"
            ], 
            "type": "object", 
            "properties": {
                "udp": {
                    "default": true, 
                    "type": "boolean", 
                    "description": "Network protocol to establish syslog session."
                }, 
                "syslog_server_port": {
                    "default": 514, 
                    "type": "integer", 
                    "description": "The destination Syslog server's service port.", 
                    "format": "int32"
                }, 
                "syslog_server": {
                    "type": "string", 
                    "description": "The destination Syslog server IP address or hostname."
                }
            }
        }, 
        "AlertSyslogConfig": {
            "required": [
                "name"
            ], 
            "type": "object", 
            "properties": {
                "syslog_servers": {
                    "items": {
                        "$ref": "#/definitions/AlertSyslogServer"
                    }, 
                    "type": "array", 
                    "description": "The list of syslog servers."
                }, 
                "uuid": {
                    "type": "string", 
                    "description": "Unique object identifier of the object."
                }, 
                "url": {
                    "readOnly": true, 
                    "type": "string", 
                    "description": "url"
                }, 
                "description": {
                    "type": "string", 
                    "description": "User defined description for alert syslog config."
                }, 
                "tenant_ref": {
                    "type": "string", 
                    "description": " It is a reference to an object of type Tenant."
                }, 
                "name": {
                    "type": "string", 
                    "description": "A user-friendly name of the syslog notification."
                }
            }
        }, 
        "AlertSyslogConfigApiResponse": {
            "required": [
                "count", 
