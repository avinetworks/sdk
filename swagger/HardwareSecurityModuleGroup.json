    "definitions": {
        "HardwareSecurityModuleGroup": {
            "required": [
                "hsm", 
                "name"
            ], 
            "type": "object", 
            "properties": {
                "_last_modified": {
                    "readOnly": true, 
                    "type": "string", 
                    "description": "UNIX time since epoch in microseconds. Units(MICROSECONDS)."
                }, 
                "uuid": {
                    "type": "string", 
                    "description": "UUID of the HSM Group configuration object."
                }, 
                "url": {
                    "readOnly": true, 
                    "type": "string", 
                    "description": "url"
                }, 
